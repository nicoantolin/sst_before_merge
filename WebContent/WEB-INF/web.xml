<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
	<display-name>sst</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<context-param>
		<param-name>log4jConfigLocation</param-name>
		<param-value>classpath:/cl/abcdin/sst/conf/log4j.xml</param-value>
	</context-param>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>classpath:/cl/abcdin/sst/conf/application-context.xml</param-value>
	</context-param>
	<listener>
		<listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
	</listener>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>dwr-invoker</servlet-name>
		<servlet-class>org.directwebremoting.servlet.DwrServlet</servlet-class>
		<init-param>
			<param-name>debug</param-name>
			<param-value>false</param-value>
		</init-param>
		<init-param>
			<param-name>allowScriptTagRemoting</param-name>
			<param-value>false</param-value>
		</init-param>
		<init-param>
			<param-name>crossDomainSessionSecurity</param-name>
			<param-value>false</param-value>
		</init-param>
		<init-param>
			<param-name>config-dwr</param-name>
			<param-value>/WEB-INF/dwrBase.xml</param-value>
		</init-param>
		<init-param>
			<param-name>config-dwr-1</param-name>
			<param-value>/WEB-INF/dwr.xml</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>dwr-invoker</servlet-name>
		<url-pattern>/dwr/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>ViewReportServlet</display-name>
		<servlet-name>ViewReportServlet</servlet-name>
		<servlet-class>cl.abcdin.sst.servlets.ViewReportServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewReportServlet</servlet-name>
		<url-pattern>/ViewReportServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>UploadFileServlet</display-name>
		<servlet-name>UploadFileServlet</servlet-name>
		<servlet-class>cl.abcdin.sst.servlets.UploadFileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadFileServlet</servlet-name>
		<url-pattern>/UploadFileServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>PDAServlet</display-name>
		<servlet-name>PDAServlet</servlet-name>
		<servlet-class>cl.abcdin.sst.servlets.PDAServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PDAServlet</servlet-name>
		<url-pattern>/PDAServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>DownloadGestionServlet</display-name>
		<servlet-name>DownloadGestionServlet</servlet-name>
		<servlet-class>cl.abcdin.sst.servlets.DownloadGestionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DownloadGestionServlet</servlet-name>
		<url-pattern>/DownloadGestionServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>dispatcher</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>classpath:/cl/abcdin/sst/conf/dispatcher-servlet.xml</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>dispatcher</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
</web-app>